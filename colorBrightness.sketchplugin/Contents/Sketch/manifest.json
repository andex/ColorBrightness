{
  "compatibleVersion": 3,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Set Brightness",
      "identifier": "setbrightness",
      "script": "setbrightness.js",
      "icon": "./Resources/setbrightness.png",
      "handlers": {
        "run": "setbrightness"
      },
      "description": "Set Brightness to select layer"
    },
    {
      "name": "Darken",
      "identifier": "darken",
      "script": "darken.js",
      "shortcut": "ctrl shift d"
    },
    {
      "name": "Lighten",
      "identifier": "lighten",
      "script": "lighten.js",
      "shortcut": "ctrl shift l"
    }
  ],
  "menu": {
    "title": "ColorBrightness",
    "items": [
      "setbrightness",
      "lighten",
      "darken"
    ]
  },
  "version": "0.2.0",
  "description": "Make color lighten or darken",
  "name": "colorBrightness",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/andex/ColorBrightness/master/.appcast.xml",
  "author": "Anashkin Andrey",
  "authorEmail": "andrey@anashkin.design"
}